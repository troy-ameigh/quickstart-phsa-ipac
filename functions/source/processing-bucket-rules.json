{
    "LambdaFunctionConfigurations": [
        {
            "LambdaFunctionArn": "arn:aws:lambda:ca-central-1:ACCOUNT:function:JOB_CREATION_FUNCTION_NAME",
            "Events": [
                "s3:ObjectCreated:*"
            ],
            "Filter": {
                "Key": {
                    "FilterRules": [
                        {
                            "Name": "prefix",
                            "Value": "source-csv/"
                        },
                        {
                            "Name": "suffix",
                            "Value": ".csv"
                        }
                    ]
                }
            }
        },
        {
            "LambdaFunctionArn": "arn:aws:lambda:ca-central-1:ACCOUNT:function:FEEDBACK_LOOP_FUNCTION_NAME",
            "Events": [
                "s3:ObjectCreated:*"
            ],
            "Filter": {
                "Key": {
                    "FilterRules": [
                        {
                            "Name": "prefix",
                            "Value": "output/"
                        },
                        {
                            "Name": "suffix",
                            "Value": ".manifest"
                        }
                    ]
                }
            }
        }        
    ]
}